<p id="notice"><%= notice %></p>

<h1>Teacher Data</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Nine hundred</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @teacher_data.each do |teacher_datum| %>
      <tr>
        <td><%= teacher_datum.first_name %></td>
        <td><%= teacher_datum.last_name %></td>
        <td><%= teacher_datum.nine_hundred %></td>
        <td><%= teacher_datum.email %></td>
        <td><%= link_to 'Show', teacher_datum %></td>
        <td><%= link_to 'Edit', edit_teacher_datum_path(teacher_datum) %></td>
        <td><%= link_to 'Destroy', teacher_datum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Teacher Datum', new_teacher_datum_path %>
