<p id="notice"><%= notice %></p>

<h1>Teacher Offices</h1>

<table>
  <thead>
    <tr>
      <th>Building name</th>
      <th>Room number</th>
      <th>Floor number</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @teacher_offices.each do |teacher_office| %>
      <tr>
        <td><%= teacher_office.building_name %></td>
        <td><%= teacher_office.room_number %></td>
        <td><%= teacher_office.floor_number %></td>
        <td><%= link_to 'Show', teacher_office %></td>
        <td><%= link_to 'Edit', edit_teacher_office_path(teacher_office) %></td>
        <td><%= link_to 'Destroy', teacher_office, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Teacher Office', new_teacher_office_path %>
